public static String angryProfessor(int k, List<Integer> a) {
        int count = 0;
        for(int i=0; i<a.size(); i++){
            
            if(a.get(i)<= 0){
                count++;
            }
        }
        
        if(count >= k){
            return "NO";
        }else{
            return "YES";
        }
    }

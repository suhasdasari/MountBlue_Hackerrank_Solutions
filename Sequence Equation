public static List<Integer> permutationEquation(List<Integer> p) {
        
        ArrayList<Integer> al = new ArrayList<>();
        
        for(int i=1; i<=p.size(); i++){
            
            int num1 = p.indexOf(i)+1;
            int num2 = p.indexOf(num1)+1;
            al.add(num2);
        }
        
        return al;
        
    }

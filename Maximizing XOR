public static int maximizingXor(int l, int r) {
        int num =0;
        
        for(int i=l; i<=r; i++){
            for(int j=l; j<=r; j++){
                int x = i^j;
                if(x>num){
                    num=x;
                }
            }
        }
        
        return num;
    }

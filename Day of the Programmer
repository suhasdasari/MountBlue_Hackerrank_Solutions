public static String dayOfProgrammer(int year) {
        String dm = "12.09.";
        String dm1 = "13.09.";
        String dmy = "";
        
        if(year < 1919 && year%4 == 0 || year>=1919 && year%400 ==0 || year>=1919 && year%4 ==0 && year%100 !=0){
            dmy = dm+year;
        }else{
            dmy = dm1+year;
        }
        
        return dmy;
    }

static String catAndMouse(int x, int y, int z) {
        
        int cat1 = Math.abs(z-x);         //checks and returns positive number i.e {  Math.abs(-1) --> returns 1  }
        int cat2 = Math.abs(z-y);         // Finding diff btw cat and mouse
        
        if(cat1<cat2){
            return "Cat A";
        }else if (cat2<cat1){
            return "Cat B";
        }else{
            return "Mouse C";
        }


    }

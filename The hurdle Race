public static int hurdleRace(int k, List<Integer> height) {
        int max =0;
            
        for(int i=0; i<height.size(); i++){
            if(height.get(i)>max){
                max = height.get(i);
            }
        }
        
        if(k>=max){
            return 0;
        }else{
            return max-k;
        }

    }
